<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/white"
    tools:context=".FifthActivity">

    <RelativeLayout
        android:id="@+id/d51"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">


            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="370sp"
                android:layout_height="283dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10sp" />

            <TextView
                android:id="@+id/classified5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView5"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10sp"
                android:text="Disease:"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/result5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/classified5"
                android:layout_centerHorizontal="true"
                android:text=""
                android:textColor="#C30000"
                android:textSize="27sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/confidencesText5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/result5"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10sp"
                android:text="Percentage:"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/confidence5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/confidencesText5"
                android:layout_centerHorizontal="true"
                android:text=""
                android:textColor="#000"
                android:textSize="25sp" />

            <Button
                android:id="@+id/e5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@id/button51"
                android:backgroundTint="@color/green"
                android:text="View More Info"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textSize="21sp"
                android:textStyle="bold"
                android:visibility="invisible" />

            <!--                <LinearLayout-->
            <!--                    android:id="@+id/align1"-->
            <!--                    android:layout_width="match_parent"-->
            <!--                    android:layout_height="149dp"-->
            <!--                    android:orientation="vertical"-->
            <!--                    android:visibility="visible" ></LinearLayout>-->

            <Button
                android:id="@+id/button51"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_above="@id/button52"
                android:layout_centerInParent="true"
                android:backgroundTint="@color/green"
                android:text="Camera"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textSize="21sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button52"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerInParent="true"
                android:backgroundTint="@color/green"
                android:text="Upload Image"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textSize="21sp"
                android:textStyle="bold" />

            <WebView
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/d52"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        tools:visibility="invisible">

        <WebView
            android:id="@+id/webview5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="invisible"
            tools:visibility="invisible" />

    </RelativeLayout>



</RelativeLayout>